            </RelativeLayout>
            <CheckBox android:id="@+id/cbAirRearWin" android:background="@drawable/ksw_benz_chk_air_rear_frost" android:clickable="false" android:layout_width="75.7dp" android:layout_height="41.6dp" android:checked="true" android:button="@null" android:layout_centerVertical="true" android:layout_marginEnd="60dp" android:layout_alignParentEnd="true"/>
