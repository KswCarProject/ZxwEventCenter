        </RelativeLayout>
    </RelativeLayout>
    <TextView android:textSize="18sp" android:textStyle="normal" android:textColor="#fff" android:gravity="center" android:id="@+id/tvAirOff" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:text="@string/lb_air_closed"/>
