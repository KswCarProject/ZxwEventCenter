<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent" android:layout_height="41dp">
    <ImageView android:background="@drawable/kesaiwei_benz_air_di" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <RelativeLayout android:id="@+id/viewAirOn" android:layout_width="match_parent" android:layout_height="match_parent">
        <RelativeLayout android:id="@+id/rlFrontWinMaxOffShow" android:layout_width="match_parent" android:layout_height="match_parent">
            <TextView android:textSize="18sp" android:textColor="#fff" android:gravity="center" android:id="@+id/tvAirLeftTemp" android:layout_width="60dp" android:layout_height="match_parent" android:text="20.0℃" app:layout_constraintEnd_toStartOf="@+id/viewWindLeft" app:layout_constraintStart_toStartOf="0"/>
            <TextView android:textSize="18sp" android:textColor="#fff" android:gravity="center" android:id="@+id/tvECO" android:layout_width="50dp" android:layout_height="match_parent" android:text="ECO" android:layout_marginStart="80dp"/>
            <RelativeLayout android:id="@+id/viewWindLeft" android:layout_width="80dp" android:layout_height="match_parent" android:layout_centerVertical="true" android:layout_marginStart="130dp">
