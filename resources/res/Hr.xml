            <CheckBox android:id="@+id/cbAircirCulat" android:background="@drawable/ksw_benz_chk_air_circulation" android:clickable="false" android:layout_width="75.7dp" android:layout_height="41.6dp" android:button="@null" android:layout_centerVertical="true" android:layout_marginEnd="210dp" android:layout_alignParentEnd="true"/>
            <RelativeLayout android:id="@+id/viewWindRight" android:layout_width="80dp" android:layout_height="match_parent" android:layout_marginEnd="130dp" android:layout_alignParentEnd="true">
                <ImageView android:background="@drawable/kesaiwei_benz_air_xiaoren" android:layout_width="41dp" android:layout_height="41dp" android:layout_centerVertical="true" android:layout_marginStart="23dp"/>
                <CheckBox android:id="@+id/cbAirRightWindUp" android:background="@drawable/ksw_benz_chk_wind_up" android:clickable="false" android:layout_width="21.3dp" android:layout_height="13.6dp" android:layout_marginTop="4dp" android:button="@null" android:layout_marginStart="6.4dp"/>
                <CheckBox android:id="@+id/cbAirRightWindParallel" android:background="@drawable/ksw_benz_chk_wind_front" android:clickable="false" android:layout_width="12.8dp" android:layout_height="9.6dp" android:layout_marginTop="14.4dp" android:button="@null" android:layout_marginStart="25.6dp"/>
