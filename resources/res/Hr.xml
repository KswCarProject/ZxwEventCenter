                <CheckBox android:id="@+id/cbAirLeftWindParallel" android:background="@drawable/ksw_benz_chk_wind_front" android:clickable="false" android:layout_width="12.8dp" android:layout_height="9.6dp" android:layout_marginTop="14.4dp" android:button="@null" android:layout_marginStart="25.6dp"/>
                <CheckBox android:id="@+id/cbAirLeftWindDown" android:background="@drawable/ksw_benz_chk_wind_down" android:clickable="false" android:layout_width="12dp" android:layout_height="12dp" android:layout_marginTop="22.4dp" android:button="@null" android:layout_marginStart="12dp"/>
                <CheckBox android:id="@+id/cbAirLeftWindAuto" android:background="@drawable/ksw_benz_chk_wind_auto" android:clickable="false" android:layout_width="39.2dp" android:layout_height="13.6dp" android:layout_marginTop="6.4dp" android:button="@null" android:layout_marginStart="4.8dp"/>
            </RelativeLayout>
            <TextView android:textSize="18sp" android:textColor="#fff" android:gravity="center" android:id="@+id/tvSync" android:visibility="visible" android:layout_width="100dp" android:layout_height="match_parent" android:text="SYNC" android:layout_centerInParent="true"/>
