        </RelativeLayout>
        <View android:id="@+id/view0" android:layout_width="100dp" android:layout_height="match_parent" android:layout_centerInParent="true"/>
        <CheckBox android:id="@+id/cbAirFrontWin" android:background="@drawable/ksw_benz_chk_air_front_frost" android:visibility="gone" android:clickable="false" android:layout_width="68dp" android:layout_height="26dp" android:checked="true" android:button="@null" android:layout_centerVertical="true" android:layout_marginStart="130dp"/>
        <RelativeLayout android:id="@+id/viewWindSpeed" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_toStartOf="@+id/view0">
            <ImageView android:id="@+id/cbAirWindSpeed" android:background="@drawable/kesaiwei_benz_air_fengshan" android:clickable="false" android:layout_width="75.7dp" android:layout_height="41.6dp" android:button="@null" android:layout_centerVertical="true"/>
            <TextView android:textSize="18sp" android:textColor="#fff" android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/tvAirWindSpeed" android:layout_width="30dp" android:layout_height="match_parent" android:text="0.0" android:layout_marginStart="50dp"/>
